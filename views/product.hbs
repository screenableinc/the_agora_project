<!--                    <a href=""><i class="fa fa-twitter"></i></a>-->
<!--                    <a href=""><i class="fa fa-youtube"></i></a>-->
<!--                </div>-->
            </div>
            <div class="col-lg-3 product-details seller-info">
                <h4 class="p-stock">Business Name: <span class="vendorname"></span></h4>
                <h4 class="p-stock">Deliverable: <span id="deliverable"></span></h4>
                <div class="rating">
                <span class="fa fa-star-o"></span>
                <span class="fa fa-star-o"></span>
                <span class="fa fa-star-o"></span>
                <span class="fa fa-star-o"></span>
                <span class="fa fa-star-o"></span>
                    <span class="p-stock" id="reviews"><a href=""><span id="count"></span> reviews</a></span>
                    
                </div>
                <button class="btn follow">Follow</button>
            </div>
        </div>
