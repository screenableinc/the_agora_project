<div id="productDetails" style="display: none">{{product}}</div>



<!-- product section -->
<section class="product-section">
    <div class="container">

        <div class="row">
            <div  class="col-lg-4">
                <div class="product-pic-zoom">
                    <img style="border-radius: 3px;" id="productImage" class="product-big-img" src={{imageurl}} alt="">
                </div>
                <div class="product-thumbs" tabindex="1" style="overflow: hidden; outline: none;">
<!--                    <div class="product-thumbs-track">-->
<!--&lt;!&ndash;                        <div class="pt active" data-imgbigurl=><img src= alt=""></div>&ndash;&gt;-->
<!--&lt;!&ndash;                        <div class="pt" data-imgbigurl="img/single-product/2.jpg"><img src="img/single-product/thumb-2.jpg" alt=""></div>&ndash;&gt;-->
